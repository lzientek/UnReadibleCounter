<UserControl x:Class="UnReadableClock.ReturnDisplayBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UnReadableClock"
             mc:Ignorable="d" 
             Height="50" Width="50">
        <Grid Height="50" Width="50">
        <Path Data="M0,100 L100,0 0,0  z" Fill="Black" HorizontalAlignment="Left" VerticalAlignment="Top" Stretch="Uniform"  Width="12" />
        <Path Data="M100,100 L0,100 100,0 z" Fill="Black" HorizontalAlignment="Right" Margin="0" Stretch="Uniform"  VerticalAlignment="Bottom" Width="12"/>
        <Path Data="M0,50 L12,50 50,12 50,0 38,0 0,38 z" Fill="Black" HorizontalAlignment="Left" Margin="0" Stretch="Uniform"  Width="50" />
    </Grid>
</UserControl>
